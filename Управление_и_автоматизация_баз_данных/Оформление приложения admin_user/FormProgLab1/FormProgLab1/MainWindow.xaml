<Window x:Class="FormProgLab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FormProgLab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Height="419" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition Height="42"/>
            <RowDefinition/>
            <RowDefinition Height="42"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="295"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Fill="#FFF4F4F5" Grid.Row="2" StrokeThickness="0"/>
        <Button x:Name="ButtonBack" Content="Назад" HorizontalAlignment="Left" Margin="10,10,0,10" Width="75" Click="ButtonBack_Click"/>
        <Frame x:Name="MainFrame" Content="Fame" Grid.Row="1" Grid.ColumnSpan="3" HorizontalAlignment="Right" Width="792" Height="335" VerticalAlignment="Bottom" Source="AuthPage.xaml" Navigated="MainFrame_OnNavigated"/>

    </Grid>
</Window>
