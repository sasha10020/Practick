<Window x:Class="Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image HorizontalAlignment="Left" Height="75" Margin="10,0,0,0" VerticalAlignment="Top" Width="75" Source="pngwing.com.png"></Image>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>
        <Button Content="Back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click" ></Button>
        <Image HorizontalAlignment="Left" Height="64" Margin="10,6,0,5.2" VerticalAlignment="Center" Width="111" Source="X5new.jpg"/>
        <TextBlock HorizontalAlignment="Center" Text="x5Group" VerticalAlignment="Center" FontSize="30" Margin="324.428,12.048,355.171,23.151"/>
    </Grid>
</Window>
